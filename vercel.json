{
  "trailingSlash": false,
  "headers": [
    {
      "source": "/login/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/blog/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/login",
      "destination": "/login/index.html"
    },
    {
      "source": "/login/",
      "destination": "/login/index.html"
    },
    {
      "source": "/login/dashboard",
      "destination": "/login/index.html"
    },
    {
      "source": "/login/calendar",
      "destination": "/login/index.html"
    },
    {
      "source": "/login/clients",
      "destination": "/login/index.html"
    },
    {
      "source": "/login/add-client",
      "destination": "/login/index.html"
    },
    {
      "source": "/login/upgrade",
      "destination": "/login/index.html"
    },
    {
      "source": "/login/:path*",
      "has": [
        {
          "type": "header",
          "key": "accept",
          "value": ".*text/html.*"
        }
      ],
      "destination": "/login/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/admin",
      "destination": "/login",
      "permanent": false
    },
    {
      "source": "/quick",
      "destination": "/core-buddy",
      "permanent": true
    }
  ]
}
